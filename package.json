{
  "name": "fit-together",
  "version": "0.0.3",
  "description": "Fit health software designed with fitness lovers in mind",
  "author": "Tegan Duong, Lynn Chen, AJ Grande, Peter Chim",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src/server -d dev/server --copy-files; webpack; npm run move-assets",
    "start": "npm run watch & nodemon dev/server/server.js & open http://localhost:3000",
    "watch": "npm run watch-c & npm run watch-s",
    "watch-c": "npm run move-assets & webpack --watch --progress",
    "watch-s": "babel --watch src/server -d dev/server --copy-files",
    "seed": "node src/server/testData/seedUsers.js; node src/server/testData/seedTeams.js; node src/server/testData/seedEntries.js",
    "lint": "eslint src --color",
    "move-assets": "cp -R src/client/assets/ dev/client/assets",
    "test": "npm run lint",
    "test-votes": "mocha --compilers js:babel-core/register --require ./tests/testHelpers.js  --recursive",
    "seed-log": "node src/server/testData/seedSleep.js; node src/server/testData/seedMem.js; node src/server/testData/seedExercise.js; node src/server/testData/seedFood.js",
    "seed-test": "node src/server/testData/seedTeams.js;",
    "json": "node src/server/testData/seedSleep.js",
    "mocha-db": "mocha "
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.4",
