{
  "name": "fit-together",
  "version": "0.0.3",
  "description": "Fit health software designed with fitness lovers in mind",
  "author": "Tegan Duong, Lynn Chen, AJ Grande, Peter Chim",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "babel src/server -d dev/server --copy-files; webpack",
    "start": "npm run watch & nodemon dev/server/server.js & open http://localhost:3000",
    "watch": "npm run watch-c & npm run watch-s",
    "watch-c": "npm run move-assets & webpack --watch --progress",
    "watch-s": "babel --watch src/server -d dev/server --copy-files",
    "seed": "node src/server/testData/seedUsers.js; node src/server/testData/seedTeams.js; node src/server/testData/seedActivities.js;",
    "lint": "eslint src --color",
    "move-assets": "cp -R src/client/assets dev/client/",
    "test": "npm run lint",
    "seed-test": "node src/server/testData/seedTeams.js"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.8.5",
    "mongodb": "^2.1.20",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2",
    "rd3": "^0.6.3",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "aws-sdk": "^2.4.0",
    "bluebird": "^3.4.0",
    "body-parser": "~1.13.2",
    "connect": "^3.4.1",
    "cookie-parser": "^1.4.3",
    "d3": "^3.5.17",
    "es6-promise": "^3.2.1",
    "express": "~4.13.1",
    "express-session": "^1.13.0",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^4.5.0",
    "nodemon": "^1.9.2",
    "passport": "^0.3.2",
    "passport-fitbit-oauth2": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-moves": "0.0.1",
    "pg": "^5.1.0",
    "pg-promise": "^4.6.4",
    "react": "^15.0.2",
    "react-d3": "^0.4.0",
    "react-d3-basic": "^1.6.11",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "~2.3.0"
  },
  "peerDependencies": {
    "react-dom": "^0.14.7"
  }
}
